{
  "id": "observer-agent",
  "name": "Observer Agent",
  "description": "Monitors the actions and messages between agents, recording important events and semantic context.",
  "version": "1.0.0",
  "author": "ABI Framework",
  "agent_type": "observer",
  "content_types": ["text/plain", "application/json", "text/markdown"],
  "entrypoint": "observer.main:app",
  "communication_protocol": "A2A-Protocol",
  "requires_context_id": true,
  "inputs": [
    "event_stream",
    "semantic_annotations",
    "agent_interactions"
  ],
  "outputs": [
    "observation_log",
    "semantic_index",
    "timeline_data"
  ],
  "prompt_reference": "prompts.OBSERVER_COT_INSTRUCTIONS",
  "tags": ["abi", "observer", "monitoring", "events", "semantics"]
}
